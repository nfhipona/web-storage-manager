{
  "name": "web-storage-manager",
  "version": "4.0.1",
  "description": "Web utility storage manager to handle save, update and data purge",
  "main": "lib/index.js",
  "type": "commonjs",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "test:build": "rm -rf lib && npm run build:js && jest",
    "build": "rm -rf lib && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nferocious76/web-storage-manager.git"
  },
  "keywords": [
    "react",
    "local",
    "storage",
    "local storage",
    "web storage"
  ],
  "author": "Neil Francis Ramirez Hipona",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nferocious76/web-storage-manager/issues"
  },
  "homepage": "https://github.com/nferocious76/web-storage-manager#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/node": "^20.4.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "core-js": "^3.31.0",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3"
  }
}