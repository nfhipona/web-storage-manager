{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "no-console": true,
        "no-debugger": true,
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ]
    }
}